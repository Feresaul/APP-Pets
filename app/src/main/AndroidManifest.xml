<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.petsapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/baseline_pets_white_48dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_pets_white_48dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Time"></activity>
        <activity android:name=".Service" />
        <activity android:name=".MyServices" />
        <activity android:name=".History" />
        <activity android:name=".Home" />
        <activity android:name=".SingUp" />
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>